{
  "name": "miid",
  "version": "6.0.0",
  "description": "A middleware and context system",
  "keywords": [
    "middleware",
    "context",
    "typescript"
  ],
  "homepage": "https://github.com/etienne-dldc/miid#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/miid/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/miid.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/miid.es.js",
      "require": "./dist/miid.cjs.js"
    }
  },
  "main": "./dist/miid.cjs.js",
  "module": "./dist/miid.es.js",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist deno_dist && vite build && yarn build:types && denoify",
    "build:types": "tsc -p ./tsconfig.build.json",
    "codecov": "codecov",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "codecov": "^3.8.3",
    "denoify": "^0.10.6",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.7",
    "jest-ts-tsx-resolver": "^1.0.0",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.12"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
