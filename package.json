{
  "name": "miid",
  "version": "6.0.2",
  "description": "A middleware and context system",
  "keywords": [
    "middleware",
    "context",
    "typescript"
  ],
  "homepage": "https://github.com/etienne-dldc/miid#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/miid/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/miid.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist deno_dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts && denoify",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "denoify": "^0.11.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "tsup": "^5.11.13",
    "typescript": "^4.6.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
